  "bayesian_inference_context": {
    "description": "Sequential Bayesian inference updates probabilities as new evidence arrives",
    "key_concepts": [
      "Prior probability: Initial belief before any evidence",
      "Posterior probability: Updated belief after observing evidence",
      "Sequential updating: Each posterior becomes the next prior",
      "Evidence accumulation: More evidence leads to stronger conclusions",
      "Bayes' Theorem: P(Disease|Symptoms) = P(Symptoms|Disease) Ã— P(Disease) / P(Symptoms)"
    ],
    "real_world_analogy": "Like a detective gathering clues - each new piece of evidence updates the theory",
    "inference_types": {
      "predictive": "Given disease, predict symptoms (forward reasoning)",
      "diagnostic": "Given symptoms, infer disease (backward reasoning)",
      "sequential": "Update diagnosis as symptoms appear over time (iterative reasoning)"
    },
    "learning_objectives": [
      "Understand sequential Bayesian inference",
      "Implement step-by-step probability updates",
      "Visualize how confidence changes with evidence",
      "Apply to real-world progressive diagnosis",
      "Compare single-step vs multi-step inference"
    ]
  }
