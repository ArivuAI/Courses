        "description": "Patient gender",
        "type": "categorical",
        "possible_values": ["Male", "Female"]
      },
      {
        "name": "ChestPain",
        "description": "Type of chest pain experienced",
        "type": "categorical",
        "possible_values": ["Typical", "Atypical", "NonAnginal", "Asymptomatic"]
      },
      {
        "name": "RestingBP",
        "description": "Resting blood pressure in mm Hg",
        "type": "numeric",
        "range": "94-200"
      },
      {
        "name": "Cholesterol",
        "description": "Serum cholesterol in mg/dl",
        "type": "numeric",
        "range": "126-564"
      },
      {
        "name": "BloodSugar",
        "description": "Fasting blood sugar > 120 mg/dl",
        "type": "categorical",
        "possible_values": ["Normal", "High"]
      },
      {
        "name": "MaxHeartRate",
        "description": "Maximum heart rate achieved during exercise",
        "type": "numeric",
        "range": "71-202"
      },
      {
        "name": "ExerciseInducedAngina",
        "description": "Exercise induced chest pain",
        "type": "categorical",
        "possible_values": ["Yes", "No"]
      },
      {
        "name": "Smoking",
        "description": "Smoking status",
        "type": "categorical",
        "possible_values": ["Yes", "No"]
      }
    ],
    "target_concept": "HeartDisease",
    "target_description": "Whether patient has heart disease risk (High/Low)",
    "target_values": ["High", "Low"]
  },
